@model IEnumerable<HRM_PLUS_PROJECT.Models.Nomina>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Monto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDeduccionNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdEmpleadoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTransaccionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Monto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDeduccionNavigation.IdDeduccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEmpleadoNavigation.IdEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTransaccionNavigation.IdTransaccion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdNomina">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdNomina">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdNomina">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
